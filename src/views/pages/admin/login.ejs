
    <%- include('../../partials/header'); -%>

    <main id="login">

        <section class="login">
            <h2 class="login__title">INGRESAR A MI CUENTA</h2>
            <div class="login__form">
                <form  method="post" id="login-form" action="" enctype="application/x-www-form-urlencoded">
                    <div class="login-form">
                        <label class="login-form__label" for="mail">Email:</label>
                        <input class="login-form__input" type="text" name="mail" id="email" required placeholder="jhondoe@correo.com"><br>

                        <label class="login-form__label" for="password">Contraseña:</label>
                        <input class="login-form__input" type="password" name="password" id="password" required placeholder="&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;&#9679;"><br>
                    </div>
                    <div class="login-submit">
                        <input type="submit" value="Ingresar">
                        <div class="remember-me">
                            <input type="checkbox" name="remember">
                            <label>Recordarme</label>
                        </div>
                    </div>
                </form>
            </div>
            <div class="forgot">
                <a href="">Olvide mi contraseña</a>
            </div>

        </section>


    </main>

    <%- include('../../partials/footer'); -%>

    <script src="https://cdn.jsdelivr.net/npm/iconify-icon@1.0.8/dist/iconify-icon.min.js"></script>

</body>
</html>